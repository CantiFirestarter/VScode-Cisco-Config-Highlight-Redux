{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Cisco Config",
  "scopeName": "source.cisco",
  "patterns": [
    { "include": "#unused_line" },
    { "include": "#comment" },
    { "include": "#string" },
    { "include": "#group" },
    { "include": "#interface_type" },
    { "include": "#vrf" },
    { "include": "#acl" },
    { "include": "#crypto" },
    { "include": "#ip_sla" },
    { "include": "#address" },
    { "include": "#arp_insp_val" },
    { "include": "#command_hostname" },
    { "include": "#numeric" },
    { "include": "#separator" },
    {
      "comment": "bgp must be placed after address. {neighbor [peer-group-name or ip-address]}",
      "include": "#bgp"
    },
    { "include": "#keyword" }
  ],
  "repository": {
    "unused_line": {
      "patterns": [{ "include": "#unused" }, { "include": "#default" }],
      "repository": {
        "unused": {
          "name": "meta.function-call.command-disable.unused.cisco",
          "match": "(?i)^\\s*?no\\s.*"
        },
        "default": {
          "name": "meta.function-call.command-disable.default.cisco",
          "match": "(?i)^\\s*?default\\s.*"
        }
      }
    },
    "comment": {
      "patterns": [
        {
          "name": "comment.line.config.cisco",
          "match": "^\\s*[!#].*"
        },
        {
          "name": "comment.line.config.cisco",
          "begin": "(?<=\\s)[!#]",
          "end": "$"
        }
      ]
    },
    "string": {
      "patterns": [
        { "include": "#hostname" },
        { "include": "#description" },
        { "include": "#remark" },
        { "include": "#domain-name" },
        { "include": "#username" },
        { "include": "#name" },
        { "include": "#logging-system-message" },
        { "include": "#password" },
        { "include": "#secret" },
        { "include": "#banner" }
      ],
      "repository": {
        "description": {
          "match": "(?i)(?<=(description))\\s(.*)$",
          "name": "string.other.description.cisco"
        },
        "remark": {
          "match": "(?i)(?<=remark)\\s(.*)$",
          "name": "string.other.remark.cisco"
        },
        "hostname": {
          "match": "(?i)(?<=hostname\\s)[0-9a-zA-Z\\-]+\\b",
          "name": "entity.name.tag.config-string.hostname.cisco"
        },
        "domain-name": {
          "match": "(?i)(^|\\s)(domain-name|domain\\sname)\\s(\\S+)\\b",
          "captures": {
            "2": {
              "name": "keyword.other.config-keyword.network-protocols.ip-routing.cisco"
            },
            "3": { "name": "entity.name.tag.config-string.domain-name.cisco" }
          }
        },
        "username": {
          "match": "(?i)(?<=username\\s)(\\S+)\\b",
          "name": "entity.name.tag.config-string.username.cisco"
        },
        "name": {
          "match": "(?i)(^|\\s)(name)\\s(\\S+)\\b",
          "captures": {
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "entity.name.tag.config-string.name.cisco" }
          }
        },
        "logging-system-message": {
          "match": "(?<=%)\\S+-\\d-\\S+(?=:)",
          "name": "entity.name.tag.config-string.logging-system-message.cisco"
        },
        "password": {
          "match": "(?i)(?<=password\\s)(?:\\d+\\s)?(\\S+)$",
          "name": "string.other.password.cisco"
        },
        "secret": {
          "match": "(?i)(?<=secret\\s)(?:\\d+\\s)?(\\S+)$",
          "name": "string.other.secret.cisco"
        },
        "banner": {
          "begin": "(?i)(?<=banner\\s(exec|login|motd)\\s)(\\^|@)",
          "end": "(\\^|@)",
          "name": "comment.block.banner.cisco"
        }
      }
    },
    "group": {
      "patterns": [{ "include": "#group-modules" }],
      "repository": {
        "group-modules": {
          "patterns": [
            { "include": "#qos-module" },
            { "include": "#routing-module" },
            { "include": "#bgp-module" },
            { "include": "#security-module" },
            { "include": "#traffic-module" },
            { "include": "#resource-module" }
          ]
        },
        "qos-module": {
          "patterns": [
            {
              "match": "(?i)(?<=class\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.class.name.cisco"
            },
            {
              "match": "(?i)(?<=service-policy\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.service-policy.name.cisco"
            },
            {
              "match": "(?i)(?<=policy-map\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.policy-map.name.cisco"
            },
            {
              "match": "(?i)(?<=class-map\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.class-map.name.cisco"
            }
          ]
        },
        "routing-module": {
          "patterns": [
            {
              "match": "(?i)(?<=route-map\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.routing.route-map.name.cisco"
            },
            {
              "match": "(?i)(?<=prefix-list\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.routing.prefix-list.name.cisco"
            }
          ]
        },
        "bgp-module": {
          "patterns": [
            {
              "match": "(?i)(?<=community\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.bgp.community.name.cisco"
            }
          ]
        },
        "security-module": {
          "patterns": [
            {
              "match": "(?i)(?<=object-group\\s)((service|network)\\s)?(\\S+)\\b",
              "captures": {
                "2": {
                  "name": "keyword.other.group.security.object-group.type.cisco"
                },
                "3": {
                  "name": "entity.name.tag.group.security.object-group.name.cisco"
                }
              }
            }
          ]
        },
        "traffic-module": {
          "patterns": [
            {
              "match": "(?i)(?<=traffic-filter\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.traffic.traffic-filter.name.cisco"
            }
          ]
        },
        "resource-module": {
          "patterns": [
            {
              "match": "(?i)(?<=dhcp\\sserver\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.resource.dhcp-server.name.cisco"
            },
            {
              "match": "(?i)(?<=\\bpool\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.resource.pool.name.cisco"
            },
            {
              "match": "(?i)(?<=policy-list\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.resource.policy-list.name.cisco"
            }
          ]
        }
      }
    },
    "interface_type": {
      "patterns": [
        { "include": "#app" },
        { "include": "#service-engine" },
        { "include": "#embedded-service-engine" },
        { "include": "#ethernet" },
        { "include": "#fibrechannel" },
        { "include": "#pos" },
        { "include": "#dpt" },
        { "include": "#lre" },
        { "include": "#token-ring" },
        { "include": "#bundle-ether" },
        { "include": "#port-channel" },
        { "include": "#multilink" },
        { "include": "#mfr" },
        { "include": "#pseudowire" },
        { "include": "#sdwan" },
        { "include": "#wireless" },
        { "include": "#management" },
        { "include": "#serial" },
        { "include": "#async" },
        { "include": "#atm" },
        { "include": "#bri" },
        { "include": "#tunnel" },
        { "include": "#lisp" },
        { "include": "#nve" },
        { "include": "#vasi" },
        { "include": "#loopback" },
        { "include": "#vlan" },
        { "include": "#bdi" },
        { "include": "#bvi" },
        { "include": "#cellular" },
        { "include": "#dialer" },
        { "include": "#virtual-template" },
        { "include": "#group-async" },
        { "include": "#null" }
      ],
      "repository": {
        "app": {
          "match": "(?i)\\b(app-?gigabitethernet|app-?g(ig)?|ap)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.app.cisco"
        },
        "async": {
          "match": "(?i)\\b(async|as)(((\\d+[\\./])*)?\\d+)\\b",
          "name": "entity.name.class.interface.async.cisco"
        },
        "atm": {
          "match": "(?i)\\b(atm)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.atm.cisco"
        },
        "bdi": {
          "match": "(?i)\\b(bdi)(\\d+)?(\\.\\d+)?\\b",
          "name": "entity.name.class.interface.bdi.cisco"
        },
        "bri": {
          "match": "(?i)\\b(bri)(((\\d+[\\./])*)?\\d+(\\:\\d+)?)\\b",
          "name": "entity.name.class.interface.bri.cisco"
        },
        "bundle-ether": {
          "match": "(?i)\\b(bundle-ether|be)(\\d+)?(\\.\\d+)?\\b",
          "name": "entity.name.class.interface.bundle-ether.cisco"
        },
        "bvi": {
          "match": "(?i)\\b(bvi)(\\d+)?(\\.\\d+)?\\b",
          "name": "entity.name.class.interface.bvi.cisco"
        },
        "cellular": {
          "match": "(?i)\\b(cellular)(((\\d+[\\./])*)?\\d+)\\b",
          "name": "entity.name.class.interface.cellular.cisco"
        },
        "dialer": {
          "match": "(?i)\\b(dialer|di)(\\d+)?\\b",
          "name": "entity.name.class.interface.dialer.cisco"
        },
        "dpt": {
          "match": "(?i)\\b(dpt)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.dpt.cisco"
        },
        "embedded-service-engine": {
          "match": "(?i)\\b(embedded-service-engine|ese)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.embedded-service-engine.cisco"
        },
        "ethernet": {
          "match": "(?i)\\b((fast|(five|ten|twentyfive|f(orty|ifty)|(two|four)?hundred)?g(ig)?(abit)?)?e(th)?(ernet)?|fa|gi|te|tf|fo|fi|hu|fh|f|g)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)(-\\d+(\\.\\d+)?)?\\b",
          "name": "entity.name.class.interface.ethernet.cisco"
        },
        "fibrechannel": {
          "match": "(?i)\\b(fc|vfc)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.fibrechannel.cisco"
        },
        "group-async": {
          "match": "(?i)\\b(group-async)(\\d+)?\\b",
          "name": "entity.name.class.interface.group-async.cisco"
        },
        "lisp": {
          "match": "(?i)\\b(lisp)(\\d+)?\\b",
          "name": "entity.name.class.interface.lisp.cisco"
        },
        "loopback": {
          "match": "(?i)\\b(loopback|lo)(\\d+)?\\b",
          "name": "entity.name.class.interface.loopback.cisco"
        },
        "lre": {
          "match": "(?i)\\b(lre)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.lre.cisco"
        },
        "management": {
          "match": "(?i)\\b(management|mgmt)(\\d+)?\\b",
          "name": "entity.name.class.interface.management.cisco"
        },
        "mfr": {
          "match": "(?i)\\b(mfr)(\\d+)?(\\.\\d+)?\\b",
          "name": "entity.name.class.interface.mfr.cisco"
        },
        "multilink": {
          "match": "(?i)\\b(multilink|mu)(\\d+)?\\b",
          "name": "entity.name.class.interface.multilink.cisco"
        },
        "null": {
          "match": "(?i)\\b(null)0\\b",
          "name": "entity.name.class.interface.null.cisco"
        },
        "nve": {
          "match": "(?i)\\b(nve)(\\d+)?\\b",
          "name": "entity.name.class.interface.nve.cisco"
        },
        "port-channel": {
          "match": "(?i)\\b(port-?channel|po)(\\d+)?(\\.\\d+)?\\b",
          "name": "entity.name.class.interface.portchannel.cisco"
        },
        "pos": {
          "match": "(?i)\\b(pos)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.pos.cisco"
        },
        "pseudowire": {
          "match": "(?i)\\b(pseudowire|pw)(\\d+)?\\b",
          "name": "entity.name.class.interface.pseudowire.cisco"
        },
        "sdwan": {
          "match": "(?i)\\b(sdwan-system-intf|sdwan|service-ext)(\\d+)?\\b",
          "name": "entity.name.class.interface.sdwan.cisco"
        },
        "serial": {
          "match": "(?i)\\b(serial|s)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b",
          "name": "entity.name.class.interface.serial.cisco"
        },
        "service-engine": {
          "match": "(?i)\\b(service-engine|se)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.service-engine.cisco"
        },
        "token-ring": {
          "match": "(?i)\\b(tokenring|tr)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.tokenring.cisco"
        },
        "tunnel": {
          "match": "(?i)\\b(tunnel|tu)(\\d+)?\\b",
          "name": "entity.name.class.interface.tunnel.cisco"
        },
        "vasi": {
          "match": "(?i)\\b(vasileft|vasiright)(\\d+)?\\b",
          "name": "entity.name.class.interface.vasi.cisco"
        },
        "virtual-template": {
          "match": "(?i)\\b(virtual-template|vt)(\\d+)?\\b",
          "name": "entity.name.class.interface.virtual-template.cisco"
        },
        "vlan": {
          "match": "(?i)\\b(vlan)(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])?\\b",
          "name": "entity.name.class.interface.vlan.cisco"
        },
        "wireless": {
          "match": "(?i)\\b(wlan-gigabitethernet|wireless-ethernet|dot11radio)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)\\b",
          "name": "entity.name.class.interface.wireless.cisco"
        }
      }
    },
    "vrf": {
      "patterns": [
        {
          "comment": "VRF declaration with optional forwarding/definition keyword",
          "match": "(?i)(?:^|\\s)(vrf)\\s(?:(forwarding)|(definition))?\\s?(\\S+)",
          "captures": {
            "1": { "name": "entity.name.class.vrf.declaration.cisco" },
            "2": { "name": "entity.other.vrf.forwarding.cisco" },
            "3": { "name": "entity.other.vrf.definition.cisco" },
            "4": { "name": "entity.name.tag.vrf.vrf-name.cisco" }
          }
        }
      ]
    },
    "acl": {
      "patterns": [
        { "include": "#access-class" },
        { "include": "#access-list" },
        { "include": "#access-group" },
        { "include": "#acl-protocols" },
        { "include": "#acl-tcp-flags" },
        { "include": "#acl-icmp-types" },
        { "include": "#acl-options" },
        { "include": "#acl-port-operators" }
      ],
      "repository": {
        "access-list": {
          "match": "(?i)(ipv6\\s+)?(ip\\s+)?(access-list)\\s+(?:(extended|standard)\\s+)?(\\S+)\\b",
          "captures": {
            "1": { "name": "keyword.other.config-keyword.routing-protocols.cisco" },
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "4": { "name": "keyword.other.acl.access-list.type.cisco" },
            "5": { "name": "entity.name.tag.acl.access-list.name.cisco" }
          }
        },
        "access-class": {
          "match": "(?i)(ipv6\\s+)?(access-class)\\s+(\\S+)(?:\\s+(in|out))?\\b",
          "captures": {
            "1": { "name": "keyword.other.config-keyword.routing-protocols.cisco" },
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "entity.name.tag.acl.access-class.name.cisco" },
            "4": { "name": "keyword.other.acl.access-class.type.cisco" }
          }
        },
        "access-group": {
          "match": "(?i)(ipv6\\s+)?((?:ip\\s+)?access-group)\\s+(\\S+)(?:\\s+(in|out))?\\b",
          "captures": {
            "1": { "name": "keyword.other.config-keyword.routing-protocols.cisco" },
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "entity.name.tag.acl.access-group.name.cisco" },
            "4": { "name": "keyword.other.acl.access-group.type.cisco" }
          }
        },
        "acl-protocols": {
          "name": "keyword.other.acl.protocol.cisco",
          "match": "(?i)\\b(ahp|esp|gre|icmpv6|igmp|ipinip|nos|pcp|pim|sctp)\\b"
        },
        "acl-tcp-flags": {
          "name": "keyword.other.acl.tcp-flag.cisco",
          "match": "(?i)\\b(ack|fin|psh|rst|syn|urg)\\b"
        },
        "acl-icmp-types": {
          "name": "keyword.other.acl.icmp-type.cisco",
          "match": "(?i)\\b(alternate-address|conversion-error|datagram-conversion-error|domain-name-reply|domain-name-request|echo-reply|echo|information-reply|information-request|ipv6-i-am-here|ipv6-where-are-you|mask-reply|mask-request|mobile-host-redirect|mobile-redirect|mobile-registration-reply|mobile-registration-request|parameter-problem|photuris|redirect|router-advertisement|router-solicitation|skip|source-quench|time-exceeded|timestamp-reply|timestamp-request|traceroute|unreachable)(?=\\s|\\b|$)"
        },
        "acl-options": {
          "match": "(?i)^(?:\\s*(?:ipv6\\s+)?(?:ip\\s+)?access-list\\b.*\\b(dynamic|evaluate|log|option|packet-length|precedence|reflect|sequence|time-range|timeout|tos|ttl)\\b|\\s*(?:permit|deny|dynamic)\\b.*\\b(dynamic|evaluate|log|option|packet-length|precedence|reflect|sequence|time-range|timeout|tos|ttl)\\b)",
          "captures": {
            "1": { "name": "keyword.other.acl.option.cisco" },
            "2": { "name": "keyword.other.acl.option.cisco" }
          }
        },
        "acl-port-operators": {
          "name": "keyword.other.acl.port-operator.cisco",
          "match": "(?i)\\b(lt|neq)\\b"
        }
      }
    },
    "crypto": {
      "patterns": [
        { "include": "#crypto-map" },
        { "include": "#transform-set" },
        { "include": "#ipsec-profile" },
        { "include": "#isakmp-profile" },
        { "include": "#keyring" }
      ],
      "repository": {
        "crypto-map": {
          "match": "(?i)(?<=\\bcrypto\\smap\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.crypto-map.name.cisco"
        },
        "transform-set": {
          "match": "(?i)(?<=\\btransform-set\\s)(\\S+\\s?)+\\b",
          "name": "entity.name.tag.crypto.transform-set.name.cisco"
        },
        "ipsec-profile": {
          "match": "(?i)(?<=\\bipsec\\sprofile\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.ipsec-profile.name.cisco"
        },
        "isakmp-profile": {
          "match": "(?i)(?<=\\bisakmp\\sprofile\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.isakmp-profile.name.cisco"
        },
        "keyring": {
          "match": "(?i)(?<=\\bkeyring\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.keyring.name.cisco"
        }
      }
    },
    "ip_sla": {
      "patterns": [
        { "include": "#ip-sla-operation" },
        { "include": "#ip-sla-monitor-operation" },
        { "include": "#ip-sla-schedule" },
        { "include": "#ip-sla-monitor-schedule" },
        { "include": "#ip-sla-group-schedule" },
        { "include": "#ip-sla-reaction-configuration" },
        { "include": "#ip-sla-logging" },
        { "include": "#ip-sla-responder" },
        { "include": "#track-ip-sla" },
        { "include": "#ip-sla-operation-line" },
        { "include": "#ip-sla-schedule-line" },
        { "include": "#ip-sla-reaction-line" }
      ],
      "repository": {
        "ip-sla-operation": {
          "match": "(?i)(?:^|\\s)(sla)\\s+(\\d+)\\b",
          "captures": {
            "1": { "name": "keyword.other.ip-sla.command.cisco" },
            "2": { "name": "entity.name.tag.ip-sla.operation-id.cisco" }
          }
        },
        "ip-sla-monitor-operation": {
          "match": "(?i)(?:^|\\s)(sla\\s+monitor)\\s+(\\d+)\\b",
          "captures": {
            "1": { "name": "keyword.other.ip-sla.command.cisco" },
            "2": { "name": "entity.name.tag.ip-sla.operation-id.cisco" }
          }
        },
        "ip-sla-schedule": {
          "match": "(?i)(?:^|\\s)(sla\\s+schedule)\\s+(\\d+)\\b",
          "captures": {
            "1": { "name": "keyword.other.ip-sla.command.cisco" },
            "2": { "name": "entity.name.tag.ip-sla.operation-id.cisco" }
          }
        },
        "ip-sla-monitor-schedule": {
          "match": "(?i)(?:^|\\s)(sla\\s+monitor\\s+schedule)\\s+(\\d+)\\b",
          "captures": {
            "1": { "name": "keyword.other.ip-sla.command.cisco" },
            "2": { "name": "entity.name.tag.ip-sla.operation-id.cisco" }
          }
        },
        "ip-sla-group-schedule": {
          "match": "(?i)(?:^|\\s)(sla\\s+group\\s+schedule)\\s+(\\S+)\\b",
          "captures": {
            "1": { "name": "keyword.other.ip-sla.command.cisco" },
            "2": { "name": "entity.name.tag.ip-sla.group-name.cisco" }
          }
        },
        "ip-sla-reaction-configuration": {
          "match": "(?i)(?:^|\\s)(sla\\s+reaction-configuration)\\s+(\\d+)\\b",
          "captures": {
            "1": { "name": "keyword.other.ip-sla.command.cisco" },
            "2": { "name": "entity.name.tag.ip-sla.operation-id.cisco" }
          }
        },
        "ip-sla-logging": {
